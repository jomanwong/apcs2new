
public class Fahrenheit_to_Celsius {
	public static void main (String[]args){
		//fahrenheit value \/\/\/\/
		double fahrenheit = 46.8;
		double celsius = (fahrenheit - 32)/9*5;
		celsius = Math.round(celsius * 10000)/10000 ;
		System.out.print("Celsius equivalent of Fahrenheit value " + fahrenheit + ":");
		System.out.print("\n" + celsius);
	}
}
