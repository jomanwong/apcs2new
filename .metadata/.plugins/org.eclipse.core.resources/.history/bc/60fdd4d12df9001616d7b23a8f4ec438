import java.io.*;
import java.util.*;

public class tempetureMonitor {
	public static void main(String[]args){
		changeInTemp();
	}
	public static void changeInTemp(){
		Scanner directory = new Scanner(new String("File directory")); //reads string of file directory
		String filePath = directory.next(); //assuming that all are numbers
		System.out.println(filePath);
	}
}
