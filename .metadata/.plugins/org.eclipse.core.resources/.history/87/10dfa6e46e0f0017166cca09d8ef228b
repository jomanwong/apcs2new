package pikachu;
import java.util.*;
//Made by Joman Wong, 8/29/16, APCS2
public class Pokemon {
	public static void main(String[]args){
			methodOne(5);
	}

	public static void methodOne(int in){
		if(in == 0){
			System.out.println();
		}else{
			System.out.print(", " + in);
			methodOne(in - 1);
		}
	}
}